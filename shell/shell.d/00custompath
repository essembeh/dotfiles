## If not running interactively, don't do anything
test -z "$PS1" && return

## PATH
export PATH="$PATH:$HOME/dotfiles/submodules/script-utils/bin"
export PATH="$PATH:$HOME/dotfiles/bin"
export PATH="$PATH:$HOME/dotfiles/bin/local"

## PYTHONPATH
export PYTHONPATH="$PYTHONPATH:$HOME/dotfiles/submodules/termicolor/"
